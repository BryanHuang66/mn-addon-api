{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "@types",
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "lib": [
      "ES2020"
    ]
  },
  "include": [
    "*.ts","@types/**/*.d.ts"
  ],
  "exclude": [
    "out.d.ts"
  ]
}
